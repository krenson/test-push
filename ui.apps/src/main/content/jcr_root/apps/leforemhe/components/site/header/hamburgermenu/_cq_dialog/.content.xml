<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Menu Hamburger"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured"
             sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <columns
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                    margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <fields jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <firstLevelPrefix jcr:primaryType="nt:unstructured"
                                              sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                              name="./firstLevelPrefix"
                                              fieldDescription="Ajouter un préfixe devant les éléments de premier niveau pour décrire leur rôle (ex: Accueil)"
                                              fieldLabel="Préfixe de premier niveau"/>
                            <notice jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/alert"
                                        jcr:title="Navigation:"
                                        text="Ce composant récupère la configuration du composant Navigation voisin."/>
                        </items>
                    </fields>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>