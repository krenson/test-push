<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Cobrowsing"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items
                        jcr:primaryType="nt:unstructured"
                        sling:hideChildren="[cloudservices,personalization,blueprint,livecopy,layout-information]">
                    <javascript
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Javascript"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fields
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <srcCobrowsing
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Source du cobrowsing"
                                            name="./src"
                                            required="{Boolean}true"/>
                                    <cbUrlCobrowsing
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="cbUrl du cobrowsing"
                                            name="./cbUrl"
                                            required="{Boolean}true"/>
                                </items>
                            </fields>
                        </items>
                    </javascript>
                    <contentCobrowsing
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Edition de contenu"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fields
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <labelLink
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Label du lien"
                                            name="./labelLink"
                                            required="{Boolean}true"/>
                                    <labelButton
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Label du button"
                                            name="./labelButton"
                                            required="{Boolean}true"/>
                                    <step1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                            fieldLabel="contenu de l'étape 1"
                                            name="./step1"
                                            required="{Boolean}true"/>
                                    <step2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                            fieldLabel="contenu de l'étape 2"
                                            name="./step2"
                                            required="{Boolean}true"/>
                                    <share
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="contenu du titre lors du partage"
                                            name="./share"
                                            required="{Boolean}true"/>
                                    <session
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="contenu du libellé de session"
                                            name="./session"
                                            required="{Boolean}true"/>

                                </items>
                            </fields>
                        </items>
                    </contentCobrowsing>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
