<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Copyright"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items
                    jcr:primaryType="nt:unstructured"
                    sling:hideChildren="[cloudservices,personalization,blueprint,livecopy,layout-information]">
                    <javascript
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Javascript"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fields
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <pathPers
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Path de l’espace-personnel"
                                        forceSelection="{Boolean}false"
                                        maxlength="255"
                                        name="./pathPers"
                                        required="{Boolean}true"
                                        rootPath="/content/leforemhe"/>
                                    <pathEntr
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Path de l’espace-entreprise"
                                        forceSelection="{Boolean}false"
                                        maxlength="255"
                                        name="./pathEntr"
                                        required="{Boolean}true"
                                        rootPath="/content/leforemhe"/>
                                    <pathPart
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Path de l’espace particulier"
                                        forceSelection="{Boolean}false"
                                        maxlength="255"
                                        name="./pathPart"
                                        required="{Boolean}true"
                                        rootPath="/content/leforemhe"/>
                                    <pathConnect
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Path de l’url de connexion"
                                        forceSelection="{Boolean}false"
                                        maxlength="255"
                                        name="./pathConnect"
                                        required="{Boolean}true"
                                        rootPath="/content/leforemhe"/>
                                    <pathDisconnect
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Path de l’url de deconnexion"
                                        forceSelection="{Boolean}false"
                                        maxlength="255"
                                        name="./pathDisconnect"
                                        required="{Boolean}true"
                                        rootPath="/content/leforemhe"/>
                                    <pathEntrGuest
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Path de l’entreprise home page HotJob"
                                        forceSelection="{Boolean}false"
                                        maxlength="255"
                                        name="./pathEntrGuest"
                                        rootPath="/content/leforemhe"/>
                                </items>
                            </fields>
                        </items>
                    </javascript>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
