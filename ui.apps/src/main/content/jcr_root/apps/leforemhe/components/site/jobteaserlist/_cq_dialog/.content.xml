<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Liste tuile"
          sling:resourceType="cq/gui/components/authoring/dialog">
  <content
    jcr:primaryType="nt:unstructured"
    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
    <items jcr:primaryType="nt:unstructured">
      <column
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
          <file
            granite:class="cmp-image__editor-file-upload"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
            class="cq-droptarget"
            fileNameParameter="./fileReferenceImage"
            fieldLabel="Fall back result image"
            fileReferenceParameter="./fallbackImage"
            mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
            name="./file"/>
          <searchPath
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
            fieldDescription="Specify search path to be used"
            fieldLabel="Search Path"
            name="./searchPaths"
            rootPath="/content"/>
        </items>
      </column>
    </items>
  </content>
</jcr:root>
